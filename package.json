{
    "name": "tree-sitter-rsx",
    "version": "0.1.5",
    "description": "The rsx language",
    "repository": "https://github.com/guobin211/tree-sitter-rsx",
    "license": "MIT",
    "author": {
        "name": "michael",
        "email": "michaelbguo@tencent.com"
    },
    "main": "./src/rsx-parser.js",
    "types": "./src/rsx-parser.d.ts",
    "keywords": [
        "incremental",
        "parsing",
        "tree-sitter",
        "rsx"
    ],
    "files": [
        "src/**",
        "syntaxes/**",
        "language-configuration.json"
    ],
    "contributes": {
        "languages": [
            {
                "id": "rsx",
                "aliases": ["RSX", "rsx"],
                "extensions": [".rsx"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rsx",
                "scopeName": "source.rsx",
                "path": "./syntaxes/rsx.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.section.rust.rsx": "rust",
                    "meta.section.script.rsx": "typescript",
                    "meta.section.style.rsx": "scss"
                }
            }
        ],
        "snippets": [
            {
                "language": "rsx",
                "path": "./syntaxes/rsx.snippets.json"
            }
        ]
    },
    "dependencies": {
        "tree-sitter": "^0.21.1",
        "tree-sitter-html": "^0.23.2",
        "tree-sitter-rust": "^0.21.0",
        "tree-sitter-scss": "^1.0.0",
        "tree-sitter-typescript": "^0.21.2"
    },
    "devDependencies": {
        "@types/node": "^22.19.1"
    },
    "scripts": {
        "dev": "tree-sitter generate && tree-sitter build --wasm",
        "test": "node --test example/test-parser.js"
    }
}
