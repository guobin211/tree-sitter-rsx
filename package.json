{
    "name": "tree-sitter-rsx",
    "version": "0.1.6",
    "description": "Tree-sitter grammar for RSX language",
    "repository": "https://github.com/guobin211/tree-sitter-rsx",
    "license": "MIT",
    "author": {
        "name": "michael",
        "email": "michaelbguo@tencent.com"
    },
    "main": "bindings/node",
    "types": "bindings/node",
    "keywords": [
        "incremental",
        "parsing",
        "tree-sitter",
        "rsx"
    ],
    "files": [
        "grammar.js",
        "binding.gyp",
        "prebuilds/**",
        "bindings/node/*",
        "queries/*",
        "src/**",
        "*.wasm"
    ],
    "tree-sitter": [
        {
            "scope": "source.rsx",
            "file-types": ["rsx"],
            "injection-regex": "^rsx$",
            "highlights": "queries/highlights.scm",
            "injections": "queries/injections.scm",
            "locals": "queries/locals.scm"
        }
    ],
    "contributes": {
        "languages": [
            {
                "id": "rsx",
                "aliases": ["RSX", "rsx"],
                "extensions": [".rsx"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "rsx",
                "scopeName": "source.rsx",
                "path": "./rsx.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.section.rust.rsx": "rust",
                    "meta.section.script.rsx": "typescript",
                    "meta.section.style.rsx": "scss"
                }
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^22.19.1",
        "tree-sitter-cli": "^0.24.0"
    },
    "scripts": {
        "build": "tree-sitter generate",
        "build-wasm": "tree-sitter build --wasm",
        "test": "tree-sitter test",
        "parse": "tree-sitter parse"
    },
    "peerDependencies": {
        "tree-sitter": "^0.21.0"
    },
    "peerDependenciesMeta": {
        "tree-sitter": {
            "optional": true
        }
    }
}
